module trans/semantics/builtins

imports
  trans/semantics/desugared-sig
  trans/semantics/values
  trans/semantics/expressions

signature
  native operators
    println : String -> String
    readln : String
    nanoTime: Long
rules
  
  SLPrintln(e) --> v 
  where
    e --> v;
    println(v2s(v)) => _.
  
  SLReadln() --> StringV(readln()).
  
  SLNanoTime() --> NumV(nanoTime()).
  
